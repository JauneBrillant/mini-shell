#!/usr/bin/env python3


def main():
    while True:
        line = input("> ")
        tokens = line.split()

        if len(tokens) == 0:
            continue

        command = tokens[0]
        args = tokens[1:]

        # TODO コマンドを実行する
        print(command)
        print(args)


if __name__ == "__main__":
    main()
